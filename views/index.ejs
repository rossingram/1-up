var minTime = 20000;
setTimeout(function() {
	var $icon = Math.random() < 0.5 ? $(".rss-icon") : $(".twitter-icon");
	var i = 10;
	var hnd = setInterval(function() {
		i--;
		if (i % 2) {
			$icon.addClass("shake1").removeClass("shake2");
		} else {
			$icon.addClass("shake2").removeClass("shake1");
		}
		if (i==0) {
			$icon.removeClass("shake1 shake2");
			clearInterval(hnd);
		}
	}, 50);
	minTime += 10000;
	setTimeout(arguments.callee, Math.random() * minTime + minTime);
}, Math.random() * minTime + minTime);
		

